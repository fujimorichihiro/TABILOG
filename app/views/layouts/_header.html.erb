<header>
  <div class="container">
    <div class="row">
      <div class="col-3">
        <h3>TABILOG</h3>
      </div>
      <div class="col-5">
      </div>
      <div class="col-4">
        <% if user_signed_in? %>
          <%= link_to user_path(current_user) do %>
            <div class="btn btn-outline-primary">My page</div>
          <% end %>
          <%= link_to destroy_user_session_path, method: :delete do %>
            <div class="btn btn-outline-primary">Log out</div>
          <% end %>
        <% else %>
          <%= link_to new_user_session_path do %>
            <div class="btn btn-outline-primary">Log in</div>
          <% end %>
          <%= link_to new_user_registration_path do %>
            <div class="btn btn-outline-primary">Sign up</div>
          <% end %>
        <% end %>
      </div>
    </div>
  </div>
</header>